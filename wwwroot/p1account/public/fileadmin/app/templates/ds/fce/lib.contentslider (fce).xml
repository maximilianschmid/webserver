									</sample_data>
									<eType>imagefixed</eType>
									<TypoScript>

10 = IMAGE
10.XY = {register:codaSliderImageWidth}, {register:codaSliderImageHeight}
10.file{
  resolution = {register:resolution}
  resolution.insertData = 1
  
  import = uploads/tx_templavoila/
  import.current = 1
  import.listNum = 0
  
  # explanation of override.dataWrap under http://typo3.toaster-schwerin.de/typo3_dev/2009_02/msg00000.html
  maxW = {register:codaSliderImageWidth}
  maxW.insertData = 1
  maxH = {register:codaSliderImageHeight}
  maxH.insertData = 1
  width = {register:codaSliderImageWidth}c
  width.insertData = 1
  height = {register:codaSliderImageHeight}c
  height.insertData = 1 
}

10.altText.field = field_imagealt
10.titleText.field = field_imagetitle

# wrap image with link deactivated
# 10.stdWrap.typolink.parameter.field = field_linkziel
									
									</TypoScript>
